<h1>Editing Meal Cost</h1>
<%= form_for(@dailyinvoice)do |f| %>
 <p>
    <%= f.label :restaurant_name %><br>
    <%= f.text_field :restaurant_name %>
  </p>
  <p>
  	<%= f.label :amount %>
	<%= f.text_field :amount %>
  </p>
  <p>
  	<%= f.label :date %>
	<%=f.text_field :date%>
   </p>
   
   <p>
   	<%= f.label :Bill %>
	<%= f.file_field :image %>
	</p>
		
   <p>
    <%= f.submit %>
  </p>
<% end %>